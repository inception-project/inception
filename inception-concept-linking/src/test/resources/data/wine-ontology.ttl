  ], [
    a owl:Restriction ;
    owl:onProperty ns0:hasFlavor ;
    owl:hasValue ns0:Strong
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:hasSugar ;
    owl:hasValue ns0:Sweet
  ] .

ns0:PinotNoir
  a owl:Class ;
