@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ns0: <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#> .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine>
  a owl:Ontology ;
  rdfs:comment "An example OWL ontology", """Derived from the DAML Wine ontology at 
      http://ontolingua.stanford.edu/doc/chimaera/ontologies/wines.daml
      Substantially changed, in particular the Region based relations.
    """ ;
  owl:priorVersion <http://www.w3.org/TR/2003/CR-owl-guide-20030818/wine> ;
  owl:imports <http://www.w3.org/TR/2003/PR-owl-guide-20031209/food> ;
  rdfs:label "Wine Ontology" .

<http://www.w3.org/TR/2003/CR-owl-guide-20030818/wine> a owl:Ontology .
<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Wine>
  a owl:Class ;
  rdfs:subClassOf <http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#PotableLiquid>, [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasMaker> ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasMaker> ;
    owl:allValuesFrom <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Winery>
  ], [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#madeFromGrape> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasSugar> ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasFlavor> ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasBody> ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasColor> ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#locatedIn> ;
    owl:someValuesFrom <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Region>
  ] ;
  rdfs:label "wine"@en, "vin"@fr .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Vintage>
  a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasVintageYear> ;
    owl:cardinality "1"^^xsd:nonNegativeInteger
  ] .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineGrape>
  a owl:Class ;
  rdfs:subClassOf <http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#Grape> .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WhiteWine>
  a owl:Class ;
  owl:intersectionOf (
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Wine>
   _:genid11
 ) .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WhiteTableWine>
  a owl:Class ;
  owl:intersectionOf (
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#TableWine>
   _:genid14
 ) .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#TableWine>
  a owl:Class ;
  owl:intersectionOf (
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Wine>
   _:genid74
 ) .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WhiteNonSweetWine>
  a owl:Class ;
  owl:intersectionOf (
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WhiteWine>
   _:genid17
 ) .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Dry> a owl:Thing, <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineSugar> .
<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#OffDry>
  a owl:Thing, <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineSugar> ;
  owl:differentFrom <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Dry>, <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Sweet> .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WhiteLoire>
  a owl:Class ;
  owl:intersectionOf (
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Loire>
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WhiteWine>
 ) ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#madeFromGrape> ;
    owl:allValuesFrom [
      a owl:Class ;
      owl:oneOf (
       <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#CheninBlancGrape>
       <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#PinotBlancGrape>
       <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#SauvignonBlancGrape>
     )
    ]
  ] .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Loire>
  a owl:Class ;
  owl:intersectionOf (
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Wine>
   _:genid240
 ) .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#CheninBlancGrape> a owl:Thing, <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineGrape> .
<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#PinotBlancGrape> a owl:Thing, <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineGrape> .
<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#SauvignonBlancGrape> a owl:Thing, <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineGrape> .
<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WhiteBurgundy>
  a owl:Class ;
  owl:intersectionOf (
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Burgundy>
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WhiteWine>
 ) ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#madeFromGrape> ;
    owl:hasValue <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#ChardonnayGrape>
  ], [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#madeFromGrape> ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
  ] .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Burgundy>
  a owl:Class ;
  owl:intersectionOf (
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Wine>
   _:genid373
 ) ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#hasSugar> ;
    owl:hasValue <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Dry>
  ] .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WhiteBordeaux>
  a owl:Class ;
  owl:intersectionOf (
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Bordeaux>
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WhiteWine>
 ) ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#madeFromGrape> ;
    owl:allValuesFrom [
      a owl:Class ;
      owl:oneOf (
       <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#SemillonGrape>
       <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#SauvignonBlancGrape>
     )
    ]
  ] .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Bordeaux>
  a owl:Class ;
  owl:intersectionOf (
   <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Wine>
   _:genid377
 ) .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#SemillonGrape> a owl:Thing, <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#WineGrape> .
<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Region> a owl:Class .
<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#locatedIn>
  a owl:ObjectProperty, owl:TransitiveProperty ;
  rdfs:domain owl:Thing ;
  rdfs:range <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Region> .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#adjacentRegion>
  a owl:ObjectProperty, owl:SymmetricProperty ;
  rdfs:domain <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Region> ;
  rdfs:range <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Region> .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#VintageYear> a owl:Class .
<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#yearValue>
  a owl:DatatypeProperty ;
  rdfs:domain <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#VintageYear> ;
  rdfs:range xsd:positiveInteger .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#Year1998>
  a <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#VintageYear> ;
  ns0:yearValue "1998"^^xsd:positiveInteger .

ns0:hasVintageYear
  a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:domain ns0:Vintage ;
  rdfs:range ns0:VintageYear .

ns0:madeFromGrape
  a owl:ObjectProperty ;
  rdfs:subPropertyOf <http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#madeFromFruit> ;
  rdfs:domain ns0:Wine ;
  rdfs:range ns0:WineGrape .

ns0:madeIntoWine
  a owl:ObjectProperty ;
  owl:inverseOf ns0:madeFromGrape .

ns0:hasWineDescriptor
  a owl:ObjectProperty ;
  rdfs:domain ns0:Wine ;
  rdfs:range ns0:WineDescriptor .

ns0:hasSugar
  a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:subPropertyOf ns0:hasWineDescriptor ;
  rdfs:range ns0:WineSugar .

ns0:hasBody
  a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:subPropertyOf ns0:hasWineDescriptor ;
  rdfs:range ns0:WineBody .

ns0:hasFlavor
  a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:subPropertyOf ns0:hasWineDescriptor ;
  rdfs:range ns0:WineFlavor .

ns0:hasColor
  a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:subPropertyOf ns0:hasWineDescriptor ;
  rdfs:domain ns0:Wine ;
  rdfs:range ns0:WineColor .

ns0:hasMaker a owl:ObjectProperty, owl:FunctionalProperty .
ns0:producesWine
  a owl:ObjectProperty ;
  owl:inverseOf ns0:hasMaker .

ns0:Zinfandel
  a owl:Class ;
  owl:intersectionOf (
   ns0:Wine
   _:genid40
   _:genid42
 ) ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ns0:hasColor ;
    owl:hasValue ns0:Red
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:hasSugar ;
    owl:hasValue ns0:Dry
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:hasBody ;
    owl:allValuesFrom [
      a owl:Class ;
      owl:oneOf (
       ns0:Full
       ns0:Medium
     )
    ]
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:hasFlavor ;
    owl:allValuesFrom [
      a owl:Class ;
      owl:oneOf (
       ns0:Moderate
       ns0:Strong
     )
    ]
  ] .

ns0:Full a owl:Thing, ns0:WineBody .
ns0:Medium a owl:Thing, ns0:WineBody .
ns0:Moderate a owl:Thing, ns0:WineFlavor .
ns0:Strong a owl:Thing, ns0:WineFlavor .
ns0:Winery a owl:Class .
ns0:WineDescriptor
  a owl:Class ;
  rdfs:comment "Made WineDescriptor unionType of tastes and color" ;
  owl:unionOf (
   ns0:WineTaste
   ns0:WineColor
 ) .

ns0:WineTaste
  a owl:Class ;
  rdfs:subClassOf ns0:WineDescriptor .

ns0:WineColor
  a owl:Class ;
  rdfs:subClassOf ns0:WineDescriptor ;
  owl:oneOf (
   ns0:White
   ns0:Rose
   ns0:Red
 ) .

ns0:White a owl:Thing, ns0:WineColor .
ns0:Rose a owl:Thing, ns0:WineColor .
ns0:Red a owl:Thing, ns0:WineColor .
ns0:WineSugar
  a owl:Class ;
  rdfs:subClassOf ns0:WineTaste ;
  owl:oneOf (
   ns0:Sweet
   ns0:OffDry
   ns0:Dry
 ) .

ns0:Sweet
  a owl:Thing, ns0:WineSugar ;
  owl:differentFrom ns0:Dry .

ns0:WineFlavor
  a owl:Class ;
  rdfs:subClassOf ns0:WineTaste ;
  owl:oneOf (
   ns0:Delicate
   ns0:Moderate
   ns0:Strong
 ) .

ns0:Delicate a owl:Thing, ns0:WineFlavor .
ns0:WineBody
  a owl:Class ;
  rdfs:subClassOf ns0:WineTaste ;
  owl:oneOf (
   ns0:Light
   ns0:Medium
   ns0:Full
 ) .

ns0:Light a owl:Thing, ns0:WineBody .
ns0:USRegion a ns0:Region .
ns0:Tours
  a owl:Class ;
  owl:intersectionOf (
   ns0:Loire
   _:genid69
 ) ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ns0:madeFromGrape ;
    owl:hasValue ns0:CheninBlancGrape
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:madeFromGrape ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
  ] .

ns0:SweetWine
  a owl:Class ;
  owl:intersectionOf (
   ns0:Wine
   _:genid77
 ) .

ns0:SweetRiesling
  a owl:Class ;
  rdfs:subClassOf ns0:DessertWine, [
    a owl:Restriction ;
    owl:onProperty ns0:hasBody ;
    owl:hasValue ns0:Full
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:hasFlavor ;
    owl:allValuesFrom [
      a owl:Class ;
      owl:oneOf (
       ns0:Moderate
       ns0:Strong
     )
    ]
  ] ;
  owl:intersectionOf (
   ns0:Riesling
   _:genid85
 ) .

ns0:Riesling
  a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ns0:hasColor ;
    owl:hasValue ns0:White
  ] ;
  owl:intersectionOf (
   ns0:Wine
   _:genid131
   _:genid133
 ) .

ns0:StEmilion
  a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ns0:hasColor ;
    owl:hasValue ns0:Red
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:hasFlavor ;
    owl:hasValue ns0:Strong
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:madeFromGrape ;
    owl:hasValue ns0:CabernetSauvignonGrape
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:madeFromGrape ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  owl:intersectionOf (
   ns0:Bordeaux
   _:genid92
 ) .

ns0:SemillonOrSauvignonBlanc
  a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ns0:hasColor ;
    owl:hasValue ns0:White
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:hasBody ;
    owl:allValuesFrom [
      a owl:Class ;
      owl:oneOf (
       ns0:Medium
       ns0:Full
     )
    ]
  ] ;
  owl:intersectionOf (
   ns0:Wine
   _:genid100
 ) .

ns0:Semillon
  a owl:Class ;
  owl:intersectionOf (
   ns0:SemillonOrSauvignonBlanc
   _:genid106
   _:genid108
 ) .

ns0:SauvignonBlanc
  a owl:Class ;
  owl:intersectionOf (
   ns0:SemillonOrSauvignonBlanc
   _:genid111
   _:genid113
 ) .

ns0:Sauternes
  a owl:Class ;
  rdfs:subClassOf ns0:LateHarvest, ns0:Bordeaux, [
    a owl:Restriction ;
    owl:onProperty ns0:locatedIn ;
    owl:hasValue ns0:SauterneRegion
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:hasBody ;
    owl:hasValue ns0:Medium
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:hasColor ;
    owl:hasValue ns0:White
  ] .

ns0:Sancerre
  a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ns0:hasBody ;
    owl:hasValue ns0:Medium
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:hasSugar ;
    owl:hasValue ns0:OffDry
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:hasFlavor ;
    owl:hasValue ns0:Delicate
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:madeFromGrape ;
    owl:hasValue ns0:SauvignonBlancGrape
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:madeFromGrape ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  owl:intersectionOf (
   ns0:Loire
   _:genid124
 ) .

ns0:RoseWine
  a owl:Class ;
  owl:intersectionOf (
   ns0:Wine
   _:genid127
 ) .

ns0:RedWine
  a owl:Class ;
  owl:intersectionOf (
   ns0:Wine
   _:genid136
 ) .

ns0:RedTableWine
  a owl:Class ;
  owl:intersectionOf (
   ns0:TableWine
   _:genid139
 ) .

ns0:RedBurgundy
  a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ns0:madeFromGrape ;
    owl:hasValue ns0:PinotNoirGrape
  ], [
    a owl:Restriction ;
    owl:onProperty ns0:madeFromGrape ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  owl:intersectionOf (
   ns0:Burgundy
   ns0:RedWine
 ) .

ns0:RedBordeaux
  a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ns0:madeFromGrape ;
