@base <http://mbugert.de/pets> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<#animal> rdf:type rdfs:Class ;
    rdfs:label "Animal" .

<#cat> rdf:type rdfs:Class ;
    rdfs:subClassOf <#animal> ;
    rdfs:label "Cat" .

<#dog> rdf:type rdfs:Class ;
    rdfs:subClassOf <#animal> ;
    rdfs:label "Dog" .

<#character> rdf:type rdfs:Class ;
    rdfs:label "Character" .

<#malicious> rdf:type <#character> ;
    rdfs:label "malicious" .

<#shy> rdf:type <#character> ;
    rdfs:label "shy" .

<#wild> rdf:type <#character> ;
    rdfs:label "wild" .

<#has-character> rdf:type rdf:Property ;
    rdfs:domain <#animal> ;
    rdfs:range <#character> ;
    skos:prefLabel "Has Character" .

<#yob> rdf:type rdf:Property ;
    rdfs:domain <#animal> ;
    rdfs:range xsd:gYear ;
    skos:prefLabel "Year Of Birth" .

<#loves> rdf:type rdf:Property ;
    rdfs:domain <#animal> ;
    rdfs:range <#animal> ;
    skos:prefLabel "Loves" .

<#hates> rdf:type rdf:Property ;
    rdfs:domain <#animal> ;
    rdfs:range <#animal> ;
    skos:prefLabel "Hates" .

<#kahmi> rdf:type <#cat> ;
    rdfs:label "Kahmi" ;
    <#has-character> <#malicious> ;
    <#yob> "2011"^^xsd:integer ;
    <#hates> <#socke> ;
    <#hates> <#lailaps> .

<#socke> rdf:type <#cat> ;
    rdfs:label "Socke" ;
    <#has-character> <#shy> ;
    <#yob> "2012"^^xsd:integer ;
    <#hates> <#kahmi> ;
    <#hates> <#lailaps> .

<#lailaps> rdf:type <#dog> ;
    rdfs:label "Lailaps" ;
    <#has-character> <#wild> ;
    <#yob> "2015"^^xsd:integer ;
    <#loves> <#kahmi> ;
    <#loves> <#socke> .
