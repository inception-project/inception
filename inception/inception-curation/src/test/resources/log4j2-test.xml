<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="ConsoleAppender" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %level{length=5} %logger{1} - %msg%n" />
    </Console>
  </Appenders>

  <Loggers>
    <Root level="WARN">
      <AppenderRef ref="ConsoleAppender" />
    </Root>
    
    <Logger name="de.tudarmstadt.ukp.inception.curation.merge.CasMergeLinkFeature" level="TRACE" />
    <Logger name="de.tudarmstadt.ukp.inception.curation.merge.CasMergeLinkTest" level="TRACE" />
  </Loggers>
</Configuration>
