<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="ConsoleAppender" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %level{length=5} %logger{1} - %msg%n" />
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="de.tudarmstadt" level="INFO"/>
<!--     <Logger name="org.springframework.security" level="TRACE" /> -->
<!--     <Logger name="org.apache.wicket.request.mapper" level="TRACE"/> -->
<!--     <Logger name="org.springframework.web.servlet" level="TRACE"/> -->
    
    <Root level="warn">
      <AppenderRef ref="ConsoleAppender" />
    </Root>
  </Loggers>
</Configuration>
