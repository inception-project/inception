{
  "realm": "inception-security-test",
  "enabled": true,
  "registrationAllowed": false,
  "verifyEmail": false,
  "loginWithEmailAllowed": false,
  "requiredCredentials": ["password"],
  "requiredActions": [
    {
      "alias": "UPDATE_PROFILE",
      "name": "Update Profile",
      "providerId": "UPDATE_PROFILE",
      "enabled": true,
      "defaultAction": false,
      "priority": 10,
      "config": {}
    },
    {
      "alias": "VERIFY_EMAIL",
      "name": "Verify Email",
      "providerId": "VERIFY_EMAIL",
      "enabled": true,
      "defaultAction": false,
      "priority": 50,
      "config": {}
    },
    {
      "alias": "UPDATE_PASSWORD",
      "name": "Update Password",
      "providerId": "UPDATE_PASSWORD",
      "enabled": true,
      "defaultAction": false,
      "priority": 30,
      "config": {}
    }
  ],
  "clients": [
    {
      "clientId": "inception-security-test-client",
      "enabled": true,
      "publicClient": false,
      "secret": "test-secret",
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": true,
      "redirectUris": ["*"],
      "protocolMappers": [
        {
          "name": "groups",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "user.attribute": "groups",
            "claim.name": "groups",
            "jsonType.label": "String",
            "multivalued": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        }
      ]
    }
  ],
  "users": [
    {
      "username": "admin-user",
      "enabled": true,
      "emailVerified": true,
      "requiredActions": [],
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "attributes": {
        "groups": ["/INCEPTION_ADMIN"]
      }
    },
    {
      "username": "regular-user",
      "enabled": true,
      "emailVerified": true,
      "requiredActions": [],
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "attributes": {
        "groups": ["/INCEPTION_USER"]
      }
    },
    {
      "username": "unknown-groups-user",
      "enabled": true,
      "emailVerified": true,
      "requiredActions": [],
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "attributes": {
        "groups": ["/UNKNOWN_GROUP"]
      }
    }
  ]
}
