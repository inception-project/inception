{
  "name": "@inception-project/${project.artifactId}",
  "version": "${semver}",
  "author": "INCEpTION Team",
  "license": "Apache-2.0",
  "description": "INCEpTION Knowledge Base UI",
  "keywords": [],
  "scripts": {
    "build": "sass scss/inception-ui-kbp.scss ../../../target/js/de/tudarmstadt/ukp/inception/ui/kb/sass/inception-ui-kbp.css",
    "build:minified": "sass scss/inception-ui-kbp.scss ../../../target/js/de/tudarmstadt/ukp/inception/ui/kb/sass/inception-ui-kbp.min.css --style compressed",
    "format": "prettier --write \"**/*.{js,ts,svelte,css,scss,json}\"",
    "format:check": "prettier --check \"**/*.{js,ts,svelte,css,scss,json}\""
  },
  "external": {},
  "overrides": {
    "minimatch": "${minimatch.version}"
  },
  "devDependencies": {
    "@inception-project/inception-support-bootstrap": "${semver}",
    "bootstrap": "${bootstrap.version}",
    "eslint": "${eslint.version}",
    "eslint-config-prettier": "${eslint-config-prettier.version}",
    "eslint-plugin-import-x": "${eslint-plugin-import-x.version}",
    "eslint-plugin-n": "${eslint-plugin-n.version}",
    "eslint-plugin-promise": "${eslint-plugin-promise.version}",
    "eslint-plugin-svelte": "${eslint-plugin-svelte.version}",
    "neostandard": "${neostandard.version}",
    "prettier": "${prettier.version}",
    "prettier-plugin-svelte": "${prettier-plugin-svelte.version}",
    "sass": "${sass.version}"
  }
}
